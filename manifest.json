{
  "manifest_version": 2,
  "name": "Controle de banco de horas",
  "description": "Aplicativo utilizado para controle de banco de horas",
  "version": "1.4",
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },
  "icons": {
    "16": "res/icon_16.png",
    "24": "res/icon_24.png",
    "128": "res/icon_128.png"
  },
  "permissions": [
    "unlimited_storage",
    "notifications"
  ],
  "web_accessible_resources": [
    "res/icon_128.png"
  ],
  "background": { "scripts": ["libs/jquery-1.9.0.min.js","js/notification.js"] }
}
